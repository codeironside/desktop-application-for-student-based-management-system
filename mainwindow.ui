       <string notr="true">/* Basic QListView styling */
QListView {
    background: #FFFFFF; /* White background */
    border: 1px solid #DDDDDD; /* Light grey border */
    border-radius: 5px; /* Rounded corners */
    padding: 5px;
    font: 12pt &quot;Segoe UI&quot;; /* Font and size */
}

/* Style for the items */
QListView::item {
    background: #FFFFFF; /* White background */
    color: #0E2433; /* Dark text color */
    padding: 10px; /* Padding for each item */
    margin: 5px 0; /* Margin between items */
    border: 1px solid #E0E0E0; /* Light grey border */
    border-radius: 3px; /* Rounded corners */
}

/* Hover state for items */
QListView::item:hover {
    background: #F1F1F1; /* Light grey background on hover */
    border: 1px solid #CCCCCC; /* Slightly darker grey border on hover */
}

/* Selected state for items */
QListView::item:selected {
    background: #BBDEFB; /* Light blue background for selected items */
    color: white; /* White text for selected items */
    border: 1px solid #BBDEFB; /* Border matches background */
}

/* Highlight selected text color */
QListView::item:selected:!active {
    color: #FFFFFF; /* White text color for inactive selected items */
}

/* Subtle shadow effect */
QListView::item {
    box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.1); /* Subtle shadow */
}
