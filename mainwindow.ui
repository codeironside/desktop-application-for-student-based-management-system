/* Tail styling using pseudo-element */
QLabel::before {
    content: &quot;&quot;;
    position: absolute;
    bottom: 10px; /* Adjust this to move the tail vertically */
    left: -10px; /* Position the tail on the left side */
    width: 0;
    height: 0;
    border: 10px solid transparent;
    border-top-color: #E3F2FD; /* Same color as the background */
    border-left: 0;
    border-right: 0;
